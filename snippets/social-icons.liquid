<ul class="list-inline social-icons">
	{% if settings.social_twitter_link != blank %}
		<li class="list-inline-item social-icon">
			<a class="social-icon-link icon-fallback-text" href="{{ settings.social_twitter_link }}" title="{{ shop.name }} on Twitter">
				<span class="icon icon-twitter" aria-hidden="true"></span>
				<span class="sr-only">Twitter</span>
			</a>
		</li>
	{% endif %}
	{% if settings.social_facebook_link != blank %}
		<li class="list-inline-item social-icon">
			<a class="social-icon-link icon-fallback-text" href="{{ settings.social_facebook_link }}" title="{{ shop.name }} on Facebook">
				<span class="icon icon-facebook" aria-hidden="true"></span>
				<span class="sr-only">Facebook</span>
			</a>
		</li>
	{% endif %}
	{% if settings.social_instagram_link != blank %}
